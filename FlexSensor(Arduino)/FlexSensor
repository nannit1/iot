const int ledPin = 3;
const int flexPin = A0;
int value;
void setup(){
pinMode(ledPin, OUTPUT);
Serial.begin(9600);
}
void loop(){
value = analogRead(flexPin);
Serial.print("sensor: ");
Serial.println(flexValue);
if(value>890){
digitalWrite(ledPin,HIGH);
Serial.println("Flex")
}
else{
digitalWrite(ledPin,LOW);
Serial.println("No flex")
}
delay(1000);
}
